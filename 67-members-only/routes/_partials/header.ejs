<header>
  <span>
    <a href="/"><%= siteName %></a>
  </span>
  <nav>
    <% if (user) {%>
      <span><%= user.first_name %> <%= user.last_name %></span>
      <a href="/auth/log-out">Log out</a>
    <% } else { %>
      <a href="/auth/log-in/view">Log in</a>
      <a href="/auth/sign-up/view">Sign up</a>
    <% } %>
  </nav>
</header>